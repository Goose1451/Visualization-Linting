{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "data": {
    "url": "data/cars.json"
  },
  "mark": {
    "type": "circle",
    "opacity": 1
  },
  "encoding": {
    "x": {
      "field": "Horsepower",
      "type": "quantitative"
    },
    "y": {
      "field": "Miles_per_Gallon",
      "type": "quantitative"
    },
    "color": {
      "field": "Origin",
      "type": "nominal",
      "scale": {
        "range": [
          "#E15658",
          "#58A14E",
          "#EDC948"
        ]
      }
    }
  }
}