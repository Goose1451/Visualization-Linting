{
  "width": 600,
  "height": 300,
  "$schema": "https://vega.github.io/schema/vega-lite/v3.0.0-rc6.json",
  "description": "A simple bar chart with ranged data (aka Gantt Chart).",
  "data": {
    "url": "./gh-specs/data/218.vl.json",
    "format": {
      "type": "csv",
      "parse": {
        "start": "date",
        "end": "date"
      }
    }
  },
  "mark": "bar",
  "encoding": {
    "y": {
      "field": "task",
      "type": "nominal",
      "sort": "null",
      "axis": {
        "title": ""
      }
    },
    "x": {
      "field": "start",
      "type": "temporal",
      "axis": {
        "title": "date",
        "format": "%b %Y",
        "tickCount": 8
      }
    },
    "x2": {
      "field": "end",
      "type": "temporal",
      "axis": {
        "title": "",
        "format": "%b %Y",
        "tickCount": 8
      }
    },
    "tooltip": [
      {
        "field": "task",
        "type": "nominal",
        "title": "activity"
      },
      {
        "field": "start",
        "type": "temporal",
        "format": "%m/%d/%Y"
      },
      {
        "field": "end",
        "type": "temporal",
        "format": "%m/%d/%Y"
      }
    ],
    "color": {
      "field": "highlight",
      "type": "nominal",
      "legend": null,
      "scale": {
        "range": [
          "#cccccc",
          "#000000"
        ]
      }
    }
  }
}