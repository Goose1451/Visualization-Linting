{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "data": {
    "url": "./gh-specs/data/24.vl.json"
  },
  "spacing": 10,
  "mark": "bar",
  "width": 130,
  "height": 100,
  "encoding": {
    "column": {
      "field": "City",
      "type": "ordinal",
      "title": ""
    },
    "y": {
      "field": "Price",
      "type": "quantitative",
      "axis": {
        "title": "Housing Sales Price",
        "grid": false
      }
    },
    "x": {
      "field": "Year",
      "type": "temporal",
      "axis": {
        "title": ""
      }
    },
    "color": {
      "field": "City",
      "type": "nominal",
      "scale": {
        "range": [
          "#CE80B0",
          "#E16167",
          "#E39158",
          "#DDC05E",
          "#F2DE8A"
        ]
      }
    }
  },
  "config": {
    "view": {
      "stroke": "transparent"
    },
    "axis": {
      "domainWidth": 1
    },
    "style": {
      "cell": {
        "stroke": "transparent"
      }
    }
  }
}