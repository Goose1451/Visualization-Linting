{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "transform": [
    {
      "filter": "datum.symbol !== 'GOOG'"
    }
  ],
  "width": 300,
  "height": 36,
  "data": {
    "url": "./gh-specs/data/67.vl.json"
  },
  "mark": "area",
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {
        "format": "%Y",
        "labelAngle": 0,
        "title": "Time",
        "grid": false
      }
    },
    "y": {
      "field": "price",
      "type": "quantitative",
      "axis": {
        "title": "Price",
        "grid": false
      }
    },
    "color": {
      "field": "symbol",
      "type": "nominal",
      "legend": null
    },
    "row": {
      "field": "symbol",
      "type": "nominal",
      "header": {
        "title": "",
        "labelAngle": -90
      }
    }
  }
}