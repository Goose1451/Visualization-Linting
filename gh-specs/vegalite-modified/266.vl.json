{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "width": 1200,
  "height": 900,
  "config": {
    "axis": {
      "labelFont": "monospace",
      "labelFontSize": 20,
      "titleFont": "cursive",
      "titleFontSize": 30,
      "titlePadding": 20
    }
  },
  "data": {
    "url": "./gh-specs/data/266.vl.json"
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "aggregate": "count",
      "type": "quantitative"
    },
    "y": {
      "field": "date",
      "type": "temporal",
      "timeUnit": "month",
      "axis": {
        "title": "Regions"
      },
      "scale": {
        "paddingInner": 0.02,
        "paddingOuter": 0
      }
    },
    "color": {
      "field": "weather",
      "type": "nominal",
      "scale": {
        "domain": [
          "0-20 days",
          "21-27 days",
          ">28 days"
        ],
        "range": [
          "red",
          "orange",
          "green"
        ]
      },
      "legend": {
        "title": "Case Ageing"
      }
    }
  }
}