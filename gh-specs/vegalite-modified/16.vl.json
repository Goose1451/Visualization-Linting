{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "transform": [
    {
      "filter": "datum.symbol !== 'GOOG'"
    }
  ],
  "width": 300,
  "height": 100,
  "data": {
    "url": "./gh-specs/data/16.vl.json"
  },
  "mark": "rect",
  "encoding": {
    "x": {
      "field": "date",
      "type": "ordinal",
      "axis": {
        "format": "%Y",
        "labelAngle": 0,
        "title": "Time"
      },
      "timeUnit": "yearmonthdate",
      "scale": {
        "paddingInner": 0
      }
    },
    "color": {
      "field": "price",
      "type": "quantitative",
      "stack": "zero",
      "aggregate": "sum",
      "legend": {
        "title": "Price"
      }
    },
    "y": {
      "field": "symbol",
      "type": "nominal",
      "axis": {
        "title": ""
      }
    }
  },
  "config": {
    "range": {
      "heatmap": {
        "scheme": "viridis",
        "extent": [
          1,
          0
        ]
      }
    }
  }
}