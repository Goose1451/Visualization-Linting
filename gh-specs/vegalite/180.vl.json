{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.0.json",
  "description": "Relationship between mileage and fare",
  "data": {
    "url": "https://raw.githubusercontent.com/F1Mider/flights/master/Amtrak.csv"
  },
  "mark": "point",
  "encoding": {
    "x": {
      "field": "Fare",
      "type": "quantitative"
    },
    "y": {
      "field": "Mileage",
      "type": "quantitative"
    }
  }
}