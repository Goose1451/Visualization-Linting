{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": {
    "url": "https://api.insa.gov.br/reservatorios/12172/monitoramento",
    "format": {
      "type": "json",
      "property": "volumes",
      "parse": {
        "DataInformacao": "utc:'%d/%m/%Y'"
      }
    }
  },
  "mark": "bar",
  "width": 400,
  "height": 300,
  "transform": [
    {
      "timeUnit": "year",
      "field": "DataInformacao",
      "as": "ano"
    }
  ],
  "encoding": {
    "x": {
      "field": "ano",
      "type": "temporal"
    },
    "y": {
      "aggregate": "count",
      "field": "ano",
      "type": "quantitative"
    }
  }
}