{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": {
    "url": "https://api.insa.gov.br/reservatorios/12172/monitoramento",
    "format": {
      "type": "json",
      "property": "volumes",
      "parse": {
        "DataInformacao": "utc:'%d/%m/%Y'"
      }
    }
  },
  "mark": "area",
  "width": 700,
  "height": 300,
  "transform": [
    {
      "filter": {
        "timeUnit": "yearmonth",
        "field": "DataInformacao",
        "range": [
          {
            "month": "jan",
            "year": 2011
          },
          {
            "month": "nov",
            "year": 2017
          }
        ]
      }
    }
  ],
  "encoding": {
    "x": {
      "field": "DataInformacao",
      "type": "temporal"
    },
    "y": {
      "aggregate": "median",
      "field": "Volume",
      "type": "quantitative"
    }
  }
}