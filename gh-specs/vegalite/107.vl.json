{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": {
    "url": "https://gist.githubusercontent.com/domoritz/1ca6a741fbfb0a64396e40f233ae6f21/raw/1b1e1e3cb5d112760fbc0fe620e1eeb6558bbdf3/delays.csv"
  },
  "width": 800,
  "height": 400,
  "mark": "rect",
  "encoding": {
    "x": {
      "field": "DepDelay",
      "type": "quantitative",
      "bin": {
        "maxbins": 50
      }
    },
    "y": {
      "field": "ArrDelay",
      "type": "quantitative",
      "bin": {
        "maxbins": 50
      }
    },
    "color": {
      "aggregate": "count",
      "type": "quantitative"
    }
  }
}