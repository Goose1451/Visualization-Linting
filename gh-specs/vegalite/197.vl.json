{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "width": 800,
  "height": 400,
  "autosize": {
    "type": "fit",
    "contains": "padding"
  },
  "layer": [
    {
      "data": {
        "url": "https://gist.githubusercontent.com/grantpezeshki/e0e48ab26efd0ee616a883c7e4e2ef5a/raw/30bd5d55219ff9956ef0e88cb7bdb166cfed51b2/notes.csv"
      },
      "mark": "rule",
      "encoding": {
        "x": {
          "field": "Date Of Note",
          "type": "temporal",
          "axis": {
            "title": "Date"
          }
        },
        "color": {
          "value": "red"
        },
        "tooltip": {
          "field": "Note Description",
          "type": "nominal"
        },
        "size": {
          "value": 2
        }
      }
    },
    {
      "data": {
        "url": "https://gist.githubusercontent.com/grantpezeshki/e0e48ab26efd0ee616a883c7e4e2ef5a/raw/30bd5d55219ff9956ef0e88cb7bdb166cfed51b2/notes.csv"
      },
      "mark": {
        "type": "text",
        "align": "center",
        "baseline": "top",
        "dx": 0,
        "dy": 5,
        "angle": 270,
        "fontWeight": "bold",
        "fontSize": 15
      },
      "encoding": {
        "x": {
          "field": "Date Of Note",
          "type": "temporal"
        },
        "text": {
          "field": "Note Title",
          "type": "nominal"
        },
        "color": {
          "value": "red"
        },
        "tooltip": {
          "field": "Note Description",
          "type": "nominal"
        }
      }
    },
    {
      "data": {
        "url": "https://gist.githubusercontent.com/grantpezeshki/e0e48ab26efd0ee616a883c7e4e2ef5a/raw/30bd5d55219ff9956ef0e88cb7bdb166cfed51b2/dailysessions.csv"
      },
      "mark": "line",
      "encoding": {
        "x": {
          "field": "Date",
          "type": "temporal"
        },
        "y": {
          "field": "Session",
          "type": "quantitative",
          "axis": {
            "title": "Sessions"
          }
        },
        "tooltip": {
          "field": "Session",
          "type": "quantitative"
        }
      }
    }
  ]
}