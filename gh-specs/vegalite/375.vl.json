{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "description": "Basic scatterplot",
  "data": {
    "url": "https://vega.github.io/vega-editor/app/data/cars.json"
  },
  "mark": "point",
  "encoding": {
    "x": {
      "field": "Horsepower",
      "type": "quantitative"
    },
    "y": {
      "field": "Miles_per_Gallon",
      "type": "quantitative"
    },
    "color": {
      "field": "Origin",
      "type": "nominal"
    }
  },
  "tooltip": {
    "field": "model",
    "type": "nominal"
  }
}