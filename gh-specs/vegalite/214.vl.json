{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": {
    "url": "https://api.insa.gov.br/reservatorios/12172/monitoramento",
    "format": {
      "type": "json",
      "property": "volumes",
      "parse": {
        "DataInformacao": "utc:'%d/%m/%Y'"
      }
    }
  },
  "vconcat": [
    {
      "mark": "bar",
      "transform": [
        {
          "filter": {
            "timeUnit": "year",
            "field": "DataInformacao",
            "range": [
              2017,
              2017
            ]
          }
        }
      ],
      "encoding": {
        "x": {
          "field": "DataInformacao",
          "timeUnit": "month",
          "type": "ordinal"
        },
        "y": {
          "aggregate": "median",
          "field": "Volume",
          "type": "quantitative",
          "scale": {
            "domain": [
              0,
              500
            ]
          }
        }
      }
    },
    {
      "mark": "bar",
      "transform": [
        {
          "filter": {
            "timeUnit": "year",
            "field": "DataInformacao",
            "range": [
              1990,
              2016
            ]
          }
        }
      ],
      "encoding": {
        "x": {
          "field": "DataInformacao",
          "timeUnit": "month",
          "type": "ordinal"
        },
        "y": {
          "aggregate": "median",
          "field": "Volume",
          "type": "quantitative",
          "scale": {
            "domain": [
              0,
              500
            ]
          }
        }
      }
    }
  ]
}